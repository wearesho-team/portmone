{
  "name": "wearesho-team/portmone",
  "description": "Portmone Integration",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "horat1us/environment-config": "^1.2",
    "wearesho-team/bobra-payments": "^1.5",
    "guzzlehttp/guzzle": "^6.3",
    "nekman/luhn-algorithm": "^4.0",
    "nesbot/carbon": "^1.33"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Bobra\\Portmone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Bobra\\Portmone\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
